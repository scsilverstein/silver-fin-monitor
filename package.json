  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register --transpile-only src/server.ts",
    "dev:all": "./run-dev.sh",
    "dev:hot": "concurrently \"npm run dev\" \"cd frontend && npm run dev\"",
    "dev:netlify": "concurrently \"npm run build:function:watch\" \"netlify dev\"",
    "dev:functions": "netlify dev",
    "dev:watch": "concurrently \"npm run build:function:watch\" \"cd frontend && npm run dev\"",
    "dev:full": "concurrently \"cd frontend && npm run dev\" \"npm run build:function:watch\"",
    "dev:netlify-complete": "netlify dev",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:auto": "node archive/root-scripts/dev-server.js",
    "build": "tsc --project tsconfig.build.json",
    "build:netlify": "node scripts/build-netlify-function.js",
    "build:function": "node scripts/build-netlify-function.js",
    "build:function:watch": "npx esbuild netlify/functions/api.ts --bundle --platform=node --outfile=netlify/functions/api.js --watch",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "db:migrate": "cd supabase && supabase db push",
    "db:reset": "cd supabase && supabase db reset",
    "whisper:setup": "./scripts/setup-whisper-m1.sh",
    "whisper:test": "python3 test-whisper.py",
    "transcription:worker": "ts-node -r tsconfig-paths/register src/services/workers/transcription.worker.ts",
    "scripts:force-analysis": "tsx scripts/force-analysis-generation.ts",
    "scripts:check-queue": "tsx scripts/check-queue-status.ts",
    "scripts:seed-feeds": "tsx scripts/seed-feeds.ts",
    "scripts:setup": "tsx scripts/setup-database.ts",
    "scripts:deploy-functions": "tsx scripts/deploy-database-functions.ts"
  },
